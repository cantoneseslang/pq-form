{
  "functions": {
    "api/**/*.mjs": { "runtime": "edge" }
  },
  "headers": [
    { "source": "/(.*)", "headers": [ { "key": "Cache-Control", "value": "no-store" } ] }
  ]
}

{
  "version": 2,
  "name": "pq-form-ui",
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "app.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}



